{
  "name": "gallery",
  "version": "1.0.0",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ionic serve",
    "ionic": "ionic serve --host IPADDRESS --configuration=en-US",
    "ionic-nor": "ionic serve --host IPADDRESS --configuration=no",
    "build": "ng build --configuration=en-US",
    "build-nor": "ng build --configuration=no",
    "test": "ng test --watch=false",
    "lint": "ng lint --fix",
    "e2e": "ng e2e",
    "i18n:extract": "ng extract-i18n --output-path src/locale",
    "prettier": "@ionic/prettier-config",
    "create-apk": "DOCKER_BUILDKIT=1 docker build --progress=plain -o output-apk .",
    "docker-ci": "docker stop ionic-temp-ci || true && docker rm ionic-temp-ci || true && docker build -t ionic-temp-ci -f docker-ci/Dockerfile . && docker rmi --force `docker images --filter dangling=true -q` || true",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/architect": "^0.1202.11",
    "@angular/common": "~13.0.1",
    "@angular/core": "~13.0.1",
    "@angular/forms": "~13.0.1",
    "@angular/platform-browser": "~13.0.1",
    "@angular/platform-browser-dynamic": "~13.0.1",
    "@angular/router": "~13.0.1",
    "@angular/service-worker": "~13.0.1",
    "@capacitor/android": "3.3.1",
    "@capacitor/app": "1.0.3",
    "@capacitor/camera": "^1.0.5",
    "@capacitor/core": "3.3.1",
    "@capacitor/filesystem": "^1.0.3",
    "@capacitor/haptics": "1.0.3",
    "@capacitor/keyboard": "1.0.3",
    "@capacitor/status-bar": "1.0.3",
    "@capacitor/storage": "^1.1.0",
    "@ionic-native/core": "^5.36.0",
    "@ionic/angular": "^5.5.2",
    "@ionic/pwa-elements": "^3.0.2",
    "@microsoft/signalr": "^5.0.10",
    "ajv": "^6.12.6",
    "cordova-plugin-insomnia": "^4.3.0",
    "prettier": "^2.4.1",
    "rxjs": "~6.6.0",
    "swiper": "^7.3.1",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.2",
    "@angular-eslint/builder": "~12.6.1",
    "@angular-eslint/eslint-plugin": "~12.6.1",
    "@angular-eslint/eslint-plugin-template": "~12.6.1",
    "@angular-eslint/template-parser": "~12.6.1",
    "@angular/cli": "~13.0.2",
    "@angular/compiler": "~13.0.2",
    "@angular/compiler-cli": "~13.0.2",
    "@angular/language-service": "~12.0.1",
    "@angular/localize": "^13.0.3",
    "@capacitor/cli": "3.2.3",
    "@cypress/schematic": "^1.5.1",
    "@ionic/angular-toolkit": "^5.0.3",
    "@ionic/prettier-config": "^2.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "cypress": "latest",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "jasmine-core": "~3.10.1",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.4.2"
  },
  "description": "An Ionic image gallery project"
}
